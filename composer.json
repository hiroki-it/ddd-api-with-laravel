{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.4",
        "aws/aws-sdk-php-laravel": "^3.0",
        "bensampo/laravel-enum": "3.0",
        "doctrine/dbal": "^2.0",
        "fideloper/proxy": "^4.0",
        "laravel/framework": "^8.0",
        "laravel/passport": "^10.0",
        "lcobucci/jwt": "^3.0"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^2.0",
        "facade/ignition": "^2.0",
        "fzaninotto/faker": "^1.0",
        "laravel/breeze": "^1.0",
        "laravel/telescope": "^4.0",
        "laravel/tinker": "^2.0",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^5.0",
        "phpunit/phpunit": "^9.0"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\Infrastructure\\DTO\\": [
                "database/factories/local",
                "database/factories/production"
            ],
            "Database\\Seeders\\": [
                "database/seeds/local",
                "database/seeds/production"
            ]
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
